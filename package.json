{
  "name": "demi-syntax",
  "displayName": "Demi-SyntaxHighlighting",
  "description": "Syntax Highlighting for the Demi Programming language",
  "publisher": "bobrossrtx",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobrossrtx/demi-syntax.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "demi",
      "aliases": ["Demi Programing Language", "demi"],
      "extensions": [".dem",".demi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "demi",
      "scopeName": "source.dem",
      "path": "./syntaxes/demi.tmLanguage.json"
    }]
  }
}
